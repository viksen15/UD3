using System;

namespace Ejercicio2
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = 5;
            double A = 4.7;
            char C = 'a';

            Console.WriteLine("{0} {1} {2}", N, A, C);
            Console.WriteLine(N + A);
            Console.WriteLine(A - N);
            Console.WriteLine((int)C);
            Console.ReadKey();

        }
    }
}
