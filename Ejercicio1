using System;

namespace Ejercicio1
{
    class Program
    {
        static void Main(string[] args)
        {
            int num1 = 4;
            int num2 = 6;

            Console.WriteLine(num1 + num2);
            Console.WriteLine(num1 - num2);
            Console.WriteLine(num1 * num2);
            Console.WriteLine(num1 / num2);
            Console.WriteLine(num1 % num2);
            Console.ReadKey();
        }
    }
}
